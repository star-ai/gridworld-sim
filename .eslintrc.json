{
    "env": {
        "jest": true
    },
    "extends": "airbnb",
    "plugins":[
        "react"
    ],
    "rules": {
        "no-plusplus": "off",
        "no-restricted-syntax": "off",
        "no-unused-vars": "warn",
        "arrow-parens": "off",
        "function-paren-newline": "off",
        "func-names": "off"
    }
}